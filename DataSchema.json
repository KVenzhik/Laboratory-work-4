{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "type": "object",
  "required": [
    "id",
    "code",
    "title"
  ],
  "properties": {
    "id": {
      "type": "integer",
      "minimum": -5
    },
    "code": {
      "type": "string",
      "pattern": "^[A-Z]+$"
    },
    "title": {
      "type": "string",
      "maxLength": 1
    }
  },
  "additionalProperties": true
}
